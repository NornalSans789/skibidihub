local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))() -- This Will Load The Script Code

-- Local
local Player = game.Players.LocalPlayer --This Will Reveal The Player Name

local loadedKey = loadstring(game:HttpGet("https://raw.githubusercontent.com/NornalSans789/key/main/skibidikey"))()
getgenv().KeyInput = "string" -- Require For The Key To Work

-- function
function MakeScriptHub() 

loadstring(game:HttpGet("https://raw.githubusercontent.com/NornalSans789/skibidihubverify/main/verifyhub"))()
end

function CheckKeyNotify()

OrionLib:MakeNotification({ 
    Name = "Checking Key", 
    Content = "Checking The Key You Entered", 
    Image = "rbxassetid://4483345998", 
    Time = 5 
})
end

print(loadedKey)


function readKey()
     if readfile("/SkibidiKey/key.txt") == loadedKey then
                  MakeScriptHub()
                  OrionLib:MakeNotification({
	Name = "Configuration Loaded!",
	Content = "Loaded the config key you previously entered",
	Image = "rbxassetid://4483345998",
	Time = 5
})
          else
                  OrionLib:MakeNotification({
	Name = "Logged In!",
	Content = "You need key "..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
})
                  local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = false, ConfigFolder = "SkibidiKey", IntroText = "Loading Hub..."}) 
                  local KeyTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

KeyTab:AddTextbox({
	Name = "Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
	    getgenv().KeyInput = Value
	end	  
})

KeyTab:AddButton({
	Name = "Check Key",
	Callback = function()
        if getgenv().KeyInput == loadedKey then 
            CheckKeyNotify()
            wait(2) 
            OrionLib:MakeNotification({ 
                Name = "Correct Key!", 
                Content = "The key you entered is Correct.", 
                Image = "rbxassetid://4483345998", 
                Time = 5 
            }) 
            wait(1) 
            OrionLib:Destroy() 
            wait(.2) 
            writefile("/SkibidiKey/key.txt", loadedKey)
            MakeScriptHub() 
       else 
           CheckKeyNotify()
           wait(2) 
           OrionLib:MakeNotification({ 
               Name = "Incorrect Key!", 
               Content = "The key you entered is incorrect.", 
               Image = "rbxassetid://4483345998", 
               Time = 5 
           }) 
       end
  	end    
})
          end
end


if isfolder("/SkibidiKey") then
     if isfile("/SkibidiKey/key.txt", "") then
         readKey()
     else
            writefile("/SkibidiKey/key.txt", "")
            readKey()
     end
else
       makefolder("SkibidiKey")
       writefile("/SkibidiKey/key.txt", "")
       readKey()
end

OrionLib:Init()
